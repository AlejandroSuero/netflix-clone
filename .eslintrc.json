{
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "next/core-web-vitals"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "@typescript-eslint/quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error"
    ],
    "@typescript-eslint/semi": [
      "error"
    ],
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}